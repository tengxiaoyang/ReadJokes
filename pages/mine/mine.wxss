/* pages/mine/mine.wxss */
.main {
  background: #F8F8F8;
  padding: 0;
  margin: 0;
  min-height: 100vh;
}
.top {
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
  width: 100%;
  height: 300rpx;
  margin: 0 0 30px 0;
}
.top-main {
  width: 40%;
  height: 300rpx;
  margin: 50px 0 12px 0;
}
.top-icon {
  display: flex;
  justify-content: center;
  width: 100%;
  height: 120rpx;
  margin: 15px 0 0 0;
}
.top-icon button {
  text-align: center;
  line-height: 226rpx;
  font-size: 42rpx;
  position: relative;
  top: -15rpx;
  width: 228rpx!important;
  height: 228rpx;
  border-radius: 114rpx;
  border: 0;
  padding: 0;
  margin: 0;
}
.top-icon-img {
  width: 120rpx;
  height: 120rpx;
  border-radius: 60rpx;
}
.top-name {
  text-align: center;
}
.top-rank {
  text-align: center;
  font-size: 12px;
  color: #f00;
}
.middle {
  background: #fff;
  margin: 0 0 30px 0;
}
.middle mp-cells mp-cell image {
  margin-right: 16px;
  vertical-align: middle;
  width:20px; 
  height: 20px;
}
.tell_a_joke {
  position: relative;
  width: 100%;
  height: 33.33%;
}
.tell_a_joke-icon_view {
  display: inline-block;
  position: relative;
  width: 100rpx;
  height: 100rpx;
}
.tell_a_joke-icon {
  position: relative;
  top: 15rpx;
  left: 25rpx;
  width: 50rpx;
  height: 50rpx;
}
.tell_a_joke-content {
  display: inline-block;
  line-height: 100rpx;
}
